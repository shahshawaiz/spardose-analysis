# Spardose Analytics Environment Configuration
# Copy this file to .env and update the values

# OpenAI Configuration
ENV=
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4.1-mini

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
